using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            try
            {
                var num1 = "1234";
                byte num2 = Convert.ToByte(num1);
                Console.WriteLine(num2);
            }

            catch(Exception)
            {
                Console.WriteLine("trying to convert larger value");
            }

        }
    }
}
